{
  "name": "axios-react-inlinesvg",
  "version": "1.0.0",
  "description": "An SVG loader for React with optional axios",
  "author": "Gil Barbara <gilbarbara@gmail.com>",
  "contributors": [
    {
      "name": "Matthew Dapena-Tretter",
      "email": "m@tthewwithanm.com"
    },
    {
      "name": "Diego Torres Fuerte",
      "email": "diego.torres.fuerte@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/gilbarbara/react-inlinesvg.git"
  },
  "bugs": {
    "url": "https://github.com/gilbarbara/react-inlinesvg/issues"
  },
  "homepage": "https://github.com/gilbarbara/react-inlinesvg#readme",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "types": "lib/index.d.ts",
  "license": "MIT",
  "keywords": [
    "react",
    "svg"
  ],
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "exenv": "^1.2.2",
    "react-from-dom": "^0.2.3"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/exenv": "^1.2.0",
    "@types/fetch-mock": "^7.3.1",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@types/node-fetch": "^2.5.0",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "chalk": "^2.4.2",
    "cross-env": "^5.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "fetch-mock": "^7.3.9",
    "http-server": "^0.11.1",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "jest-chain": "^1.1.2",
    "jest-enzyme": "^7.0.2",
    "jest-extended": "^0.11.2",
    "jest-watch-typeahead": "^0.3.1",
    "node-fetch": "^2.6.0",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^2.6.3",
    "size-limit": "^1.3.8",
    "start-server-and-test": "^1.9.1",
    "styled-components": "^4.3.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.3",
    "yargs": "^13.3.0"
  },
  "scripts": {
    "build": "npm run clean && npm run build:ts",
    "build:ts": "tsc --outDir lib",
    "watch": "npm run build:ts -- -w",
    "clean": "rimraf lib",
    "start": "http-server -p 1337 --cors test/__fixtures__",
    "test": "start-server-and-test start http://localhost:1337 test:coverage",
    "test:coverage": "jest --coverage --bail",
    "test:watch": "jest --watch --verbose",
    "lint": "tslint 'src/**/*.ts?(x)' 'test/**/*.ts?(x)' 'tools/**/*.ts?(x)'",
    "format": "prettier \"**/*.{js,jsx,json,yml,yaml,css,less,scss,ts,tsx,md,graphql,mdx}\" --write",
    "validate": "npm run lint && npm run test && npm run build && npm run bundlesize",
    "bundlesize": "size-limit",
    "prepublishOnly": "npm run validate"
  },
  "browserslist": [
    "> 0.2%",
    "safari >= 9",
    "ios >= 9"
  ],
  "prettier": {
    "jsxBracketSameLine": false,
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "size-limit": [
    {
      "path": "./lib/index.js",
      "limit": "8 KB"
    }
  ],
  "husky": {
    "hooks": {
      "post-merge": "ts-node tools update",
      "pre-commit": "ts-node tools has-commits && npm run validate"
    }
  }
}
